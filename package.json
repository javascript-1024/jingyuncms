{
  "name": "jingyun-cms-server",
  "version": "1.0.0",
  "description": "jingyun_cms",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently npm:nodeDev npm:gulpDev",
    "nodeDev": "cross-env APP_NODE_ENV=dev nodemon index.js --exec babel-node",
    "gulpDev": "cross-env APP_NODE_ENV=dev gulp build",
    "test": "concurrently npm:nodeTest npm:gulpTest",
    "nodeTest": "cross-env APP_NODE_ENV=test nodemon index.js --exec babel-node",
    "gulpTest": "cross-env APP_NODE_ENV=test gulp build",
    "prod": "npm run build && cross-env APP_NODE_ENV=prod node ./dist/index.js",
    "build": "cross-env APP_NODE_ENV=prod webpack --config webpack.config.js && cross-env APP_NODE_ENV=prod gulp build",
    "buildStart": "npm run build && cross-env APP_NODE_ENV=prod node ./dist/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "autoprefixer": "^10.4.2",
    "ejs": "^3.1.6",
    "joi": "^17.6.0",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-ejs": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.1.1",
    "koa-session": "^6.2.0",
    "koa-static": "^5.0.0",
    "koa-views": "^8.0.0",
    "nodemon": "^2.0.15",
    "svg-captcha": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "html-loader": "^3.1.0",
    "postcss": "^8.4.7",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.6",
    "sass": "^1.49.9",
    "template-ejs-loader": "^0.8.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}
